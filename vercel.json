{
  "version": 2,
  "name": "music-album",
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/.*",
      "dest": "server.js",
      "methods": ["GET,HEAD,PUT,PATCH,POST,DELETE"]
    }
  ],
  "REWRITES": [
    {
      "SOURCE": "/(.*)",
      "DESTINATION": "/"
    }
  ]
}
